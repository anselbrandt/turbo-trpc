{
  "name": "turbo-trpc",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "cleanWeb": "cd apps/web && rm -rf .next .turbo node_modules",
    "cleanDocs": "cd apps/web && rm -rf .next .turbo node_modules",
    "cleanSuper": "cd apps/web && rm -rf node_modules",
    "cleanUi": "cd apps/web && rm -rf node_modules",
    "cleanTurbo": "rm -rf node_modules yarn.lock",
    "clean": "yarn cleanWeb && yarn cleanDocs && yarn cleanSuper && yarn cleanUi && yarn cleanTurbo",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "latest",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.23.0-20220130.1630"
}
