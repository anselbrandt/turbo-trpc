{
  "name": "turbo-trpc",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "gen": "prisma generate",
    "push": "prisma db push",
    "studio": "prisma studio",
    "cleanNative": "cd apps/native && rm -rf .expo .turbo node_modules",
    "cleanWeb": "cd apps/web && rm -rf .next .turbo node_modules",
    "cleanDocs": "cd apps/docs && rm -rf .next .turbo node_modules",
    "cleanSuper": "cd packages/_superjson && rm -rf node_modules",
    "cleanUi": "cd packages/ui && rm -rf node_modules",
    "cleanEslint": "cd packages/eslint-config-custom && rm -rf node_modules",
    "cleanApi": "cd packages/api && rm -rf node_modules",
    "cleanTurbo": "rm -rf node_modules yarn.lock",
    "clean": "yarn cleanNative && yarn cleanWeb && yarn cleanDocs && yarn cleanSuper && yarn cleanUi && yarn cleanEslint && yarn cleanApi && yarn cleanTurbo",
    "clean-install": "yarn clean && yarn install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "eslint-config-custom": "latest",
    "prettier": "latest",
    "prisma": "^3.15.2",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2"
  },
  "packageManager": "yarn@1.23.0-20220130.1630"
}
